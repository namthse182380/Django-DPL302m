{
    "version": 2,
    "builds": [
      {
        "src": "deepfake_detection/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "runtime": "3.10",
            "maxLambdaSize": "15mb"
          }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "deepfake_detection/wsgi.py"
      }
    ]
  }
  